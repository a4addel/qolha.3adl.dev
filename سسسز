-- Enable the pg_trgm extension if not already enabled
CREATE EXTENSION IF NOT EXISTS pg_trgm;

-- Your fuzzy search query
SELECT *
FROM "Word"
WHERE similarity(unvocalized, 'گفتن') > 0.7
ORDER BY similarity(unvocalized, 'گفتن') DESC;

